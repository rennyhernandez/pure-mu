#{addScriptRemote "https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"}
$(function(){
    $("a.delete").click(function(){
        $.ajax({
            type: "DELETE",
            url: $(this).attr("data-url"),
        }).done(function( msg ) {
            window.location.href = window.location.href;
        });
        return false;
    });
});
